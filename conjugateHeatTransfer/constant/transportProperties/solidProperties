// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

rho             rho [ 1 -3 0 0 0 0 0 ] 2719; // Density of aluminum [kg/m3]
Cp              Cp [ 0 2 -2 -1 0 0 0 ] 896; // Specific heat capacity of aluminum [J/(kg.K)]
kappa           kappa [ 1 1 -3 -1 0 0 0 ] 235; // Thermal conductivity of aluminum [W/(m.K)]

